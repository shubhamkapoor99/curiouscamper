<%- include("Partials/header") %>

<div class="container">
<header class="jumbotron">
    <div class="container">
    <h1 style="padding-bottom: 20px;" >Campgrounds Page</h1>
    <a class="btn btn-primary btn-large" href="campgrounds/new">Add New Campground</a>





</div>
</header>

<div class="row text-center" style="display:flex; flex-wrap:wrap;">
    <% c.forEach(function(c){%>
        <div style="padding-top: 40px;" class="col-md-3 col-sm-6">
        <div class="img-thumbnail"  >
            <img class="img-fluid" src="<%= c.image %>" >
            <div class="caption">
                <h4>Name: <%=c["name"] %></h4>
            </div>
            <p>
                <a href="/campgrounds/<%= c._id%>" class="btn btn-primary">More Info</a>
            </p>
        </div>
    
</div>
<%}); %>
</div>
<%- include("partials/footer") %>
</div>